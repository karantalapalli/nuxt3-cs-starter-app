    "async": "^3.2.4",
    "babel-eslint": "^10.1.0",
    "contentstack": "^3.15.2",
    "core-js": "^3.23.1",
    "eslint": "^8.17.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-nuxt": "^3.2.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^9.1.1",
    "husky": "^8.0.1",
